<%= form_with(url: edit_dog_path, scope: :session, local: true, method: "get") do |f| %>
  <div class="field-button-fixed">
    <%= f.submit "Edit Dog", class: "font-valera", style: "width: 100%; height: 100%; background: transparent; border: none;" %>
  </div>
<% end %>

<%= form_with(url: logout_path, scope: :session, local: true, method: "delete") do |f| %>
  <div class=" field-button-fixed">
    <%= f.submit "Log Out", class: "font-valera", style: "width: 100%; height: 100%; background: transparent; border: none;" %>
  </div>
<% end %>